{
  "expo": {
    "name": "RentorraiOS",
    "slug": "rentorraios",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "icon": "./src/assets/icon.png",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Allow $(Rentorra) to use Face ID."
      },
      "bundleIdentifier": "com.dantecady.rentorraios"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/splash.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(Rentorra) to use Face ID."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "5b2d43c9-9c18-46a3-9fce-7d4638883f00"
      }
    },
    "owner": "dantecady"
  }
}
